{
  "compilerOptions": {
      "baseUrl": ".",
      "target": "esnext",
      "module": "esnext",
      "moduleResolution": "node",
      "jsx": "preserve",
      "lib": ["esnext", "dom"],
      // "types": ["jest", "node"],
      "rootDir": ".",
      "allowJs": false,
      "noUnusedLocals": true,
      "strictNullChecks": true,
      "noImplicitAny": true,
      "noImplicitThis": true,
      "experimentalDecorators": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
  },
  "include": ["__tests__"]
}
